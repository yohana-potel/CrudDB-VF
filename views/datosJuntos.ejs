<!DOCTYPE html>
<html>

<%- include('./head.ejs') %>

    <body>
        <%- include('./navigation.ejs') %>
       
        <h1>
            <%= title %>
        </h1>

        <div class="container ">
            <div class="row">
                <div class="col-md-7">
                    <table class="table table-dark table-bordered table-hover">
                        <thead>
                            <tr>
                              <td>Nombre</td>
                              <td>Email</td>
                              <td>Denominacion</td>
                            </tr>
                        </thead>
                        <tbody>
                            <% personas.forEach(element => { %>
                                <tr>
                                    <td><%= element.nombre %></td>
                                    <td><%= element.email %></td>
                                    <td><%= element.denominacion %></td>
                                    
                                </tr>
                                
                                <% }) %>
                        </tbody>
                        
                    </table>
                   

                </div>
                
                </div>
            </div>
        </div>